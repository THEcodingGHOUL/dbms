    <bean id="studentdao" class="com.robin.StudentdaoImpl"></bean>
    <bean id="teacherdao" class="com.robin.TeacherdaoImpl"></bean>
    <bean id="subjectdao" class="com.robin.SubjectdaoImpl"></bean>
    <bean id="coursedao" class="com.robin.CoursedaoImpl"></bean>
    <bean id="examdao" class="com.robin.ExamdaoImpl"></bean>
    <bean id="feedbackdao" class="com.robin.FeedbackdaoImpl"></bean>
    <bean id="examresultdao" class="com.robin.StudentResultdaoImpl"></bean>
    <bean id="blogdao" class="com.robin.BlogdaoImpl"></bean>
    <bean id="blogcommentsdao" class="com.robin.BlogcommentsdaoImpl"></bean>
    <bean id="teachingdao" class="com.robin.teachingdaoImpl"></bean>
    <bean id="jdbcTemplate" class="org.springframework.jdbc.core.JdbcTemplate">
        <property name="dataSource" ref="dataSource" />
